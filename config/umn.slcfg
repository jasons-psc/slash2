# See slcfg(5) for more information.

set port=9078;
set nets="tcp4";

set pref_mds="burton@UMN";
set pref_ios="sense51@UMN";
set fsuuid="";

site @UMN {
	site_id		= 0;

	resource burton {
		type	= mds;
		id	= 1;
		nids	= burton.umn.edu;
		journal	= /dev/sdl;
		zpool_name = "nrao_s2md";
		zpool_cache = "/opt/umn.s2/md.zcf";
	}

	resource io0 {
		type	= standalone_fs;
		id	= 10;
		nids	= umn.edu;
		fsroot	= /data/supercell0;
#		selftest = "/local/sbin/st /samfs_slio";
	}
}
